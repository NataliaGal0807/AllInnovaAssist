{
  "expo": {
    "name": "AllInnovaAssist",
    "slug": "allinnovaassist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-circular-remove.png",
    "scheme": "allinnovaassist",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.natalia.g0807.allinnovaassist",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-circular-remove.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo-circular-remove.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-circular-remove.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6c0eff40-7e3a-4c0e-b061-e12799d9e48d"
      }
    }
  }
}